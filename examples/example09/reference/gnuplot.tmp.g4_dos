set encoding iso_8859_15
set terminal postscript enhanced solid color "Helvetica" 20
set output "output_dos.ps.g4"

set key off
xmin=      -0.000001
xmax=     551.000000
set xrange [xmin:xmax]
set border lw 2
eref=0.0
fact=1.0
gfact=1.0
set ylabel "DOS (states / cm^{-1} / cell)"
set xlabel "frequency (cm^{-1})"
plot  "output_dos.dat.g4" u 1:($2*fact-eref)*gfact w l lw 3 lc rgb "red"
